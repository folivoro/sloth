{
    "name": "sixmonkey/sloth",
    "authors": [
        {
            "name": "Ben Kremer",
            "email": "hallo@benkremer.de"
        }
    ],
    "require": {
        "bordoni/phpass": "*",
        "brain/hierarchy": "*",
        "cakephp/utility": "*",
        "corcel/acf": "*",
        "friendsofphp/php-cs-fixer": "*",
        "illuminate/console": "11.8.0",
        "illuminate/container": "11.8.0",
        "illuminate/events": "11.8.0",
        "illuminate/filesystem": "11.8.0",
        "illuminate/http": "11.8.0",
        "illuminate/log": "11.8.0",
        "illuminate/pagination": "11.8.0",
        "illuminate/support": "11.8.0",
        "illuminate/validation": "11.8.0",
        "illuminate/view": "11.8.0",
        "jgrossi/corcel": "*",
        "jjgrainger/posttypes": "^2.0",
        "jover/singleton": "^1.0",
        "kdyby/html-validator-panel": "^1.1",
        "league/climate": "^3.2",
        "milo/vendor-versions": "^1.1",
        "nikic/fast-route": "^1.2",
        "nofutur3/tracy-gitpanel": "^0.9.4",
        "org_heigl/hyphenator": "^2.2",
        "phpowermove/docblock": "*",
        "roots/wp-password-bcrypt": "^1.0",
        "spatie/emoji": "*",
        "spatie/image": "*",
        "staudenmeir/belongs-to-through": "*",
        "tracy/tracy": "2.7.5",
        "twig/twig": "*",
        "vlucas/phpdotenv": "^2.4"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.11"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "composer/installers": true
        }
    },
    "extra": {
        "hooks": {
            "post-merge": [
                "composer install --ignore-platform-reqs"
            ],
            "pre-commit": [
                "vendor/bin/php-cs-fixer fix",
                "composer normalize"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Sloth\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": "cghooks add --ignore-lock",
        "post-update-cmd": "cghooks update"
    }
}
